{
  "name": "@commercelayer/commercelayer-seeder-cli",
  "version": "0.0.0",
  "description": "Commerce Layer Seeder CLI",
  "author": "Alessandro Casazza",
  "repository": "",
  "license": "MIT",
  "main": "./lib/index.js",
  "bin": {
    "commercelayer-seeder": "bin/commercelayer-seeder"
  },
  "keywords": [
    "cli",
    "commercelayer-seeder",
    "commercerlayer",
    "seeder",
    "typescript",
    "javascript",
    "nodejs"
  ],
  "scripts": {
    "start": "npm run build && npm run global",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "mocha src/**/*.spec.ts",
    "lint": "tslint -p . --fix",
    "build": "npm run clean:some && tsc -p . && tsc-alias",
    "global": "npm i -g",
    "clean:some": "rm -rf ./lib",
    "clean:all": "rm -rf ./node_modules ./package-lock.json ./lib ./docs",
    "refresh": "npm run clean:all && npm install",
    "tscov": "tscov --min-coverage 90",
    "tscov:d": "tscov --details",
    "publish-package": "npm run build && npm publish",
    "docs": "typedoc --out docs ./src",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "publish:patch": "npm run build && npm run version:patch && git push --follow-tags && npm publish --access public",
    "publish:minor": "npm run build && npm run version:minor && git push --follow-tags && npm publish --access public",
    "publish:major": "npm run build && npm run version:major && git push --follow-tags && npm publish --access public"
  },
  "dependencies": {
    "@commercelayer/commercelayer-seeder": "^1.5.3",
    "clear": "^0.1.0",
    "commander": "^7.0.0",
    "figlet": "^1.5.0",
    "fs-extra": "^9.1.0",
    "inquirer": "^7.3.3",
    "inversify": "^5.0.5",
    "kleur": "^4.1.4",
    "lodash": "^4.17.20",
    "progress-estimator": "^0.3.0",
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.1.0",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "@liftr/tscov": "^2.0.0",
    "@types/clear": "^0.1.1",
    "@types/figlet": "^1.2.1",
    "@types/fs-extra": "^9.0.7",
    "@types/inquirer": "^7.3.1",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.27",
    "@types/node-emoji": "^1.8.1",
    "@types/validator": "^13.1.3",
    "ts-node": "^9.1.1",
    "tsc-alias": "^1.2.6",
    "typedoc": "^0.20.24",
    "typescript": "^4.1.5"
  }
}
